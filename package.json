{
  "name": "smile2emoji",
  "version": "2.0.0",
  "description": "Plugin to convert from text smile to emoticons. Emoji from punctuation",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "files": [
    "lib/",
    "index.d.ts"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib --extensions \".ts,.tsx\"",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emish89/smile2emoji"
  },
  "keywords": [
    "emoji",
    "npm",
    "module",
    "node",
    "emoticons",
    "smile",
    "smiley",
    "text",
    "punctuation",
    "javascript"
  ],
  "author": "Federico Ballarini <emish89@yahoo.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emish89/smile2emoji/issues"
  },
  "homepage": "https://github.com/emish89/smile2emoji#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.3.4",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
